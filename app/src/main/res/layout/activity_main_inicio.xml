<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondouniversidad"
    tools:context=".MainActivity_Inicio">

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="121dp"
        android:layout_height="128dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="120dp"
        android:onClick="goToCalendar"
        app:srcCompat="@mipmap/calendario" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="121dp"
        android:layout_height="128dp"
        android:layout_marginLeft="235dp"
        android:layout_marginTop="120dp"
        android:onClick="goToStudents"
        app:srcCompat="@mipmap/estudiantes" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="121dp"
        android:layout_height="128dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="370dp"
        android:onClick="goToLessons"
        app:srcCompat="@mipmap/libros" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="121dp"
        android:layout_height="128dp"
        android:layout_marginLeft="235dp"
        android:layout_marginTop="370dp"
        android:onClick="exit"
        app:srcCompat="@mipmap/salida" />

</RelativeLayout>